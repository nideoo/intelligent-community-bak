<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.enc.dao.mapper.DoorsMsMapper" >

    <insert id="addMsDoors" parameterType="com.enc.domain.access.entity.DoorsMsEntity" >
        insert into t_doors_ms (id,doorTyp,`name`,status,path,X,Y)
        values (
            #{id},#{doorTyp},#{name},#{status},#{path},#{xzb},#{yzb}
        )
    </insert>

    <select id="getCountById" parameterType="java.lang.String" resultType="java.lang.Integer">
        select count(*) cou from t_doors_ms where id = #{id};
    </select>

</mapper>














