<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.enc.dao.mapper.DevicesMsMapper" >

    <insert id="addMsDevice" parameterType="com.enc.domain.access.entity.DevicesMsEntity" >
        insert into t_devices_ms (id,deviceTyp,`name`,devicePath)
        values (
            #{id},#{deviceTyp},#{name},#{devicePath}
        )
    </insert>
</mapper>














